<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.fragments.HomeFragment"
    android:orientation="vertical"
    android:background="@color/white">


    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentCenter"
        android:name="com.project.healthcare.ui.home.fragments.center_fragment.HomeCenterFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentBottom"
        android:name="com.project.healthcare.ui.home.fragments.bottom_fragment.HomeBottomFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</androidx.appcompat.widget.LinearLayoutCompat>